  "version": "0.2.0",
  "configurations": [
    {
      "name": "DeSmuMe",
      "type": "gdb",
      "request": "attach",
      "executable": "${workspaceFolder}/build/hello.elf",
      "target": ":10000",
      "remote": true,
      "cwd": "${workspaceRoot}",
      "gdbpath": "arm-none-eabi-gdb",
      "preLaunchTask": "desmume-debug",
      "postDebugTask": "desmume-debug-cleanup"
    }
  ]
}
